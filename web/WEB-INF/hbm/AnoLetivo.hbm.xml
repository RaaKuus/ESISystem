<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="esi.system.model.AnoLetivo"
           table="ano_letivo">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="anoLetivo" type="string">
            <column name="ano_letivo" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>