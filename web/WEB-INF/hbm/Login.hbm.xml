<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="esi.system.model.Login" table="login">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="cpf" column="cpf" type="string"/>
        <property name="senha" column="senha" type="string"/>
        <property name="salt" column="salt" type="string"/>
        <property name="permissoes" column="permissoes" type="string"/>
        
    </class>
</hibernate-mapping>
