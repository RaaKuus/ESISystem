<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="esi.system.model.AlunoAnoLetivo" table="aluno_ano_letivo">
        <id name="alunosMatricula" type="int" column="alunos_matricula"/>
        <property name="turmasId" column="turmas_id" type="int"/>
        <property name="tipo" column="tipo" type="string"/>
    </class>
</hibernate-mapping>