<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="esi.system.model.Aluno"
           table="alunos">
        <id name="matricula" type="int">
            <column name="matricula" />
        </id>
        <property name="nome" type="stromg">
            <column name="aluno" not-null="false" />
        </property>
        
        <property name="sexo" type="int">
            <column name="sexo" not-null="false" />
        </property>
        
        <property name="naturalidade" type="string">
            <column name="naturalidade" not-null="false" />
        </property>
        
        <property name="dataNascimento" type="java.sql.Timestamp">
            <column name="nascimento" not-null="false" />
        </property>
        
        <property name="endereco" type="string">
            <column name="endereco" not-null="false" />
        </property>
        
        <property name="nomePai" type="string">
            <column name="nomePai" not-null="false" />
        </property>
        
        <property name="identidadePai" type="string">
            <column name="identidadePai" not-null="false" />
        </property>
        
        <property name="nomeMae" type="string">
            <column name="nomeMae" not-null="false" />
        </property>
        <property name="identidadeMae" type="String">
            <column name="identidadeMae" not-null="false" />
        </property>
        <property name="cor" type="int">
            <column name="cor" not-null="false" />
        </property>
        <property name="" type="">
            <column name="" not-null="false" />
        </property>
    </class>
</hibernate-mapping>